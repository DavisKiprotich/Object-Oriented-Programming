function Dog(name) {
  this.name = name;
}

let beagle = new Dog("Snoopy");

Dog.prototype.isPrototypeOf(beagle);  // yields true.This shows that Dog is a supertype while beagle is a subtype

// Object is a supertype of both Dog and beagle .It returns true
Object.prototype.isPrototypeOf(Dog.prototype);
